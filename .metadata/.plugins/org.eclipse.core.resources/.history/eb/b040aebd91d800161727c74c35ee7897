package com.Basics3;

 class Person
{
	 public static void main(String[] args)//main method in class name holding same name as source file hence JVM looks for the main in a class with same source file name

	{
		System.out.println("Main from person");
	}
}
 class Manager
{
	
	public static void main(String[] args)//to use this class main method the source filename should be same as of class name
	{
		System.out.println("Main from Manager");
	}
}
